@inproceedings{clemente2007mapping,
  title={Mapping large loops with a single hand-held camera},
  author={Clemente, Laura A and Davison, Andrew J and Reid, Ian and
                  Neira, Jos{\'e} and Tard{\'o}s, Juan D},
  booktitle={Robotics: Science and Systems},
  year={2007}
  note={This article talks about how to identify features in
                  an individual frame of a camera, and then match
                  relative change across the frame.  They had graphs
                  with lines of best fit for each frame and then they
                  matched lines of best fit for loop closure.  Didn't
                  see the exact process though, they have some mock
                  code in the back as well.}
